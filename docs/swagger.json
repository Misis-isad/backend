                "fio",
                "password"
            ],
            "properties": {
                "email": {
                    "type": "string",
                    "format": "email",
                    "example": "test@test.ru"
                },
                "fio": {
                    "type": "string",
                    "example": "Мисосов Мисос Мисосович"
                },
                "password": {
                    "type": "string",
                    "example": "test"
                }
            }
